networks:
  main:
    name: mapeenetwork
services:
  mapentitiesservice:
    build:
      context: C:\Git\MapeeCleanCodeProject\MicroServices
      dockerfile: MapEntitiesService/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MapEntitiesService
      target: base
    container_name: MapEntitiesService
    depends_on:
      messageBrokerService:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: mapentitiesservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/MapEntitiesService.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      main: {}
    ports:
    - published: 50001
      target: 80
    - target: 80
    restart: unless-stopped
    tty: true
    volumes:
    - C:\Git\MapeeCleanCodeProject\MicroServices\MapEntitiesService:/app:rw
    - C:\Users\roi08\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\roi08\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\roi08\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\roi08\.nuget\packages:/root/.nuget/packages:ro
    - C:\Git\MapeeCleanCodeProject\MicroServices:/src:rw
  messageBrokerService:
    container_name: RabbitMqMessageBroker
    image: rabbitmq:3-management
    networks:
      main: {}
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
  seqLoggerService:
    container_name: SeqLogger
    environment:
      ACCEPT_EULA: "Y"
    image: datalust/seq:latest
    networks:
      main: {}
    ports:
    - published: 50002
      target: 80
    - published: 5341
      target: 5341
    volumes:
    - c:\Docker\Data\Seq:/data:rw
version: '3.4'